[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[[source]]
url = "https://download.pytorch.org/whl/cu118"
verify_ssl = true
name = "downloadpytorch"

[packages]
gitpython = "*"
matplotlib = "*"
numpy = "*"
Pillow = "*"
psutil = "*"
PyYAML = "*"
requests = "*"
scipy = "*"
thop = "*"
